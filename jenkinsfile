pipeline {
    agent any
    options {
        buildDiscarder(logRotator(numToKeepStr: '5', artifactNumToKeepStr: '', daysToKeepStr: '', artifactDaysToKeepStr: ''))
        disableConcurrentBuilds()
    }
    stages {
        stage('Hello') {
            steps {
                echo 'Hello Fix bug1'
            }
        }
        stage('cat README') {
            when {
                branch 'fix-*'
            }
            steps {
                sh 'cat README.md'
            }
        }
    }
}